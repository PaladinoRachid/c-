#include <iostream>
#define TAM 51

using namespace std;

void imprime (char string[])
{
    int i;
    for (i = 0; string[i]!='\0'; i++)
    {
        if(string[i]!=' ')
        {
            cout << string[i];
        }
    }
}

int main()
{
    char string [TAM-1];
    cin.getline(string,TAM);
    imprime(string);
    return 0;
}
