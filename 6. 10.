#include <iostream>
#define cidade 25000

using namespace std;

int urna (int votos[])
{
    int i;
    int c = 0;
    
    for( i = 0; i < cidade; i++)
    {
        cin>> votos[i];
        if (votos[i]< 0)
        {
            break;
        }
        
        c++;
    }
    return c;
}

void tse (int c, int votos[])
{
    int i, anula = 0, can1 = 0, can2 = 0, can3 = 0, can4 = 0, bran = 0;
    for (i = 0; i < c; i++)
    {
        if (votos [i] > 4 )
        {
            anula++;
            continue;
        }
        switch ( votos[i])
        {
            case 1: can1++;
            break;
            
            case 2: can2++;
            break;
            
            case 3: can3++;
            break;
            
            case 4: can4++;
            break;
            
            default: bran++;
            
        }
    }
    cout << "Total de votos: "<<c<< " voto(s)"<< endl;
    
    cout << "Votos do candidato 1: "<<can1<<" voto(s)"<< endl;
    
    cout <<  "Votos do candidato 2: "<< can2<<" voto(s)"<< endl;

    cout << "Votos do candidato 3: "<< can3 <<" voto(s)"<< endl;

    cout << "Votos do candidato 4: "<< can4<< " voto(s)"<< endl;

    cout << "Votos em branco: "<< bran<< " voto(s)"<< endl;
    
    cout << "Votos nulos: " << anula << " voto(s)"<< endl;
}

int main()
{
    int votos[cidade];
    int c;
    c = urna(votos);
    tse(c, votos);

    return 0;
}
