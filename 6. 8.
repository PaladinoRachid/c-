#include <iostream>
#define size 10

using namespace std;

int main()
{
    int vetorA[size];
    int i, j, k, t, c = 0;
    
    for (i = 0; i < size; i++)
    {
        cin >> vetorA[i];
    }
    
    for ( i = 0; i < size; i++)
    {
        for (j = 0; j < size; j++)
        {
            if (vetorA[i]== vetorA[j])
            {
                if (i != j)
                {
                    if ( c == 0)
                    {
                        k = i;
                        t = j;
                        c++;
                    }
                }

            }
        }
    }
    
    if (c == 1)
    {
        cout << "Par de elementos repetidos: "<< k << " e "<< t;
    }
    else 
    {
        cout << "Nao ha elementos repetidos no vetor.";
    }
    
    return 0;
}
