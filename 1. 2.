#include <iostream>

using namespace std;

int main()
{ 
    //Variáveis
    int a, b;
    
    //Entrada
    cin >> a;
    cin >> b;
    
    //Saída com operações
    cout << a << " + " << b << " = " << a + b << endl;
    cout << a << " - " << b << " = " << a - b << endl;
    cout << a << " * " << b << " = " << a*b << endl;
    cout << a << " / " << b << " = " << a/b << endl;
    cout << a << " % " << b << " = " << a%b << endl;
    
    return 0;
}
