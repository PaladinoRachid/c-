#include <iostream>

using namespace std;

int main()
{
    float x;
    int c;
    float maior = -10000000000, qmaior, menor = 100000000,  qmenor ;
    
    for ( c = 1; c <= 10; c++)
    {
        cin >> x;
        
        if (x > maior)
        {
        maior = x;
        qmaior = c;
        }
        
        if (x < menor)
        {
        menor = x;
        qmenor = c;
        }
    
    }
    
    cout << "O "<<qmaior<<"o elemento é o maior." << endl; 
    cout << "O "<<qmenor<<"o elemento é o menor."<< endl;
    

    return 0;
}
