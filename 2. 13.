#include <iostream>
#include <iomanip>

using namespace std;

float media(float a, float b, float c)
{ 
    float m;
    m = (a+b+c)/3;
    return m;
}    
int main()
{
    float a, b, c, m;
    cin >> a >> b >> c;
    m = media(a,b,c);
    cout << fixed << setprecision(2) << "MÃ©dia: " << m;

    return 0;
}
