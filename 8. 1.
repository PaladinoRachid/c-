#include <iostream>
#include <iomanip>
#define LIN 4
#define COL 2

using namespace std;

int main()
{
    int i, j;
    
    cout<< fixed << setprecision(1);
    
    float mat[LIN][COL];
    for (i = 0; i < LIN; i++)
    {
        for( j = 0; j < COL; j++)
        {
            cin >> mat[i][j];
        }
    }
    for (i = 0; i < LIN; i++)
    {
        if( i != 0)
        cout<< endl;
        
        for( j = 0; j < COL; j++)
        {
            cout<< mat[i][j]<< " ";
        }
    }

    return 0;
}
