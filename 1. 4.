#include <iostream>
#include <cmath>

using namespace std;

int main()  
{ 
   //Variáveis
   float a, b, c, MaiorAB, MaiorGeral;
   //Entrada
   cin >> a;
   cin >> b;
   cin >> c;
   //Operações
   MaiorAB = (a+b+abs(a - b))/2;
   MaiorGeral = (MaiorAB + c + abs(MaiorAB - c))/2;
   //Saída
   cout << MaiorGeral << " eh o maior";
   
    return 0;
}
