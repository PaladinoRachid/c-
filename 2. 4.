#include <iostream>
#include <iomanip>

using namespace std;

float um(int a, int b, float c)
{
   float s;
   s = b*c;
   cout << "NUMBER = " << a << endl;
   cout << fixed << setprecision(2) << "SALARY = U$ ";
   
   return s;
}

int main()
{
    int a, b;
    float c;
    cin >> a >> b >> c;
    cout << um(a,b,c);
    
}
