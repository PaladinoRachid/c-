#include <iostream>

using namespace std;

int mor (int a, int b, int c)
{
    int m;
    if (a > b)
    { 
        if(a > c)
        { 
            m = a;
        }
        else 
        {
           m = c;
        }
    }    
    else 
    {
        if (b > c)
        {
            m = b;
        }
        else
        {
           m = c;
        }
    }
    return m;
}
int main()
{
    int a, b, c;
    cin >> a >> b >>c;
    cout<< "O maior valor digitado pelo usuario foi " << mor(a,b,c) << ".";

    return 0;
}
