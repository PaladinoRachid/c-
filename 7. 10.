#include <iostream>
#define TAM 500

using namespace std;

void sete(char v[])
{
    int i ;
    
    if(v[0]!= ' ')
    {
        cout<< v[0];
    }
    
    for ( i = 1; v[i] != '\0'; i++ )
    {
        if ( v[i] != ' ' && v[i-1]==' ')
        {
            cout << v[i];
        }
    }
    
    cout << endl;
    
    return;
}

int main()
{
    int q;
    char vetor[TAM];
    
    cin >> q;
    cin.ignore();
    
    int i = 0;
    
    while( i < q)
    {
        
        cin.getline(vetor, TAM-1);
        
        sete(vetor);
        
        
        i++;
        
    }
}
