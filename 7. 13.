#include <iostream>
#include <cstring>
#define TAM 500

using namespace std;

void soletra(char string[])
{
    int i;
    
    for(i = 0; string[i] != '\0'; i++)
    {
        cout << string[i];
        if(string[i] != ' ' && string [i+1] != ' ' )
        {
        if(string[i+1]!='\0')
        {
            cout << '-';
        }
        }
    }
}

int main()
{
    char vetor[TAM];
    
    cin.getline(vetor, TAM-1);
    
    soletra(vetor);
    
    return 0;
}
