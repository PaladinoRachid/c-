#include <iostream>

using namespace std;

int somanp( int x, int y);

int main()
{
    int x, y;
    cin >> x >> y; 
    cout << "A soma e "<< somanp(x, y);
}
int somanp( int x, int y)// 1 e 3
{
    int s = 0;
    if (x >= y) // 3 1 
    {
        y = y +1;
        while ( y < x)
        {
            if (y%2 != 0) 
            {
                s = s + y;
            }
            y++;
        }
    }
    else
    {
        x = x +1;
        while ( x < y)
        {
            if (x%2 != 0) 
            {
                s = s + x; 
            }
            x++;
        }
    }
    return s;
}
