#include <iostream>

using namespace std;

void escada (int n)
{
    int t;
    t = 1;
    
    while ( t <= n)
    {
         cout << t << " ";
         t++; 
    }
}
int main()
{
    int n, m;
    cin >> m;
    n = 1;
    while (n <= m)
    {
        escada (n);
        cout << endl;
        n++;
    }

    return 0;
}
