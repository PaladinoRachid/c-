#include <iostream>
#include <cstring>
#define TAM 500

using namespace std;


void decodi(char s1[], char s2[])
{
    int n, i;
    n = strlen(s1);
    n = n-1;
    
    for (i = 0; i <= n; i++)
    {
        s2[n - i] =  s1[i];
    }
    
    s2[n+1] = '\0';
    
    for ( i = 0; s2[i]!= '\0'; i++)
    {
        if (s2[i] == '@')
        {
            s2 [i] = 'a';
        }
    
    }
}



int main()
{
    char s1 [TAM];
    char s2 [TAM];
    
    cin.getline(s1, TAM-1);
    
    decodi(s1, s2);
    
    cout << s2;

    return 0;
}
