#include <iostream>

using namespace std;

void seila(int vetor[], int numero) 
{
    for (int i = 4; i >= 0; i--) 
    {
        vetor[i] = numero % 10;
        numero = numero/10;
    }
}

int main() 
{
    int vetor[5];
    int numero;

    cin >> numero;
    if ( numero < 0 || numero > 99999)
    {
        cout << "Entrada fora do limite estipulado";
    }
    else
    {

    seila(vetor, numero);

    cout << "Vetor obtido: ";
    cout << "{";
    for (int i = 0; i < 5; i++) 
    {
        cout << vetor[i];
        if ( i < 4)
        cout <<",";
    }
    cout << "}";
    }
    
    return 0;
}
